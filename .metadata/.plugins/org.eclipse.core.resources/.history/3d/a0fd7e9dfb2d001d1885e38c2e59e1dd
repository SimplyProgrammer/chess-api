
package org.ugp.api.chess;

import org.ugp.api.chess.engine.ChessPiece;
import org.ugp.api.chess.engine.SimpleChessEngine;

import io.javalin.Javalin;

public class HelloWorld 
{
	public static void main(String[] args) {
		var app = Javalin.create().start(8888);
		int[] i = {0};
		
		
		SimpleChessEngine engine = new SimpleChessEngine();
		app.get("/", ctx -> ctx.result("Hello World " + ++i[0]));
	}
}
