package org.ugp.api.chess.enginev2;

public class Queen extends ChessPiece
{
	public Queen(SimpleChessEngine board, int color, int x, int y) {
		super(board, "q", color, x, y);
	}
	
	@Override
	public boolean super.canMoveTo(x, y, checkIfKingInCheck) {
		return super.canMoveTo(x, y, checkIfKingInCheck) && (canMoveStraight(x, y) || canMoveDiagonal(x, y));
	}
}
