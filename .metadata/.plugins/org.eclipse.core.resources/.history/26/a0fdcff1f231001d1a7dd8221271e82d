package org.ugp.api.chess.enginev2;

public class Rook extends ChessPiece
{
	public Rook(SimpleChessEngine board, int color, int x, int y) {
		super(board, "r", color, x, y);
	}
	
	@Override
	public boolean super.canMoveTo(x, y, checkIfKingInCheck) {
		return super.canMoveTo(x, y, checkIfKingInCheck) && canMoveStraight(x, y);
	}
}