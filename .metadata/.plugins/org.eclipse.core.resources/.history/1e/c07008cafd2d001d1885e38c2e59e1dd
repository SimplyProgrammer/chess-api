package org.ugp.api.chess.engine;

public class Pawn extends ChessPiece {

	public Pawn(int color) {
		super(color, "p");
	}
	
	@Override
	public int[][] getMovmentMetrix(/*int fromX, int fromY, int toX, int toY*/) {
		int[][] metrix = new int[8][8];
		for (int x = 0; x < metrix.length; x++) {
			for (int y = 0; y < metrix[x].length; y++) {
				
			}
		}
	}

}
